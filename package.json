{
  "author": "Farhad Nowzari",
  "name": "vue-nerdy-terminal",
  "version": "0.1.8",
  "keywords": [
    "terminal", "vue", "nerdy", "advanced", "easy-terminal"
  ],
  "main": "dist/VueNerdyTerminal.umd.js",
  "files": [
    "dist/*",
    "src/types/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farhadnowzari/vue-nerdy-terminal.git"
  },
  "scripts": {
